{
    "name": "unixconverter",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": ["storage"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["dayjs.js", "browser-polyfill.min.js", "content.js"],
            "css": ["main.css"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Unix Converter"
    },
    "web_accessible_resources": ["timezones.json"]
}
